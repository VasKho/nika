  *);
  [ <sc_element sys_idtf="concept_operation">operation</sc_element> that is not a <sc_element sys_idtf="concept_function">function</sc_element> ] (*
    <- lang_en;;
    => nrel_format: format_html;;
  *);;

nrel_inclusion -|> (concept_nondeterministic_operation <= concept_function);;

