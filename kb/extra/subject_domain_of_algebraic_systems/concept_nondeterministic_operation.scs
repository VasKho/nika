=> nrel_main_idtf:
  [ недетерминированная операция ] (* <- lang_ru;; *);
  [ nondeterministic operation ] (* <- lang_en;; *);

=> nrel_idtf:
  [ операция ввода ] (* <- lang_ru;; *);
